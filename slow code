function setup() {
  createCanvas(400, 400);
  colorMode(HSB, width, height, 100);
}

function draw() {
  var x = random(width);
  var y = random(height);
	 for (var x = random(width); x < 100; x = x + 100);
 for (var y = random(height); y < 100; y = y + 100);

  stroke(x, y, 100);
  point(x, y);
}
